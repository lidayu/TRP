<my:DataSelectWindowBase x:Class="MessageBoxTEST.DataSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:my="clr-namespace:MessageBoxTEST.DataProviderBase"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DataSelectWindow" Height="300" Width="300" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="226*" />
            <RowDefinition Height="35*" />
        </Grid.RowDefinitions>
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="181,6,0,0" Name="button1" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="button1_Click" />
        <TreeView Height="200" HorizontalAlignment="Left" Margin="12,12,0,0" Name="treeView1" VerticalAlignment="Top" Width="120" ClipToBounds="True">
            <TreeViewItem Header="中国">
                <TreeViewItem Header="北京">
                    <TreeViewItem Header="东城" />
                    <TreeViewItem Header="西城" />
                </TreeViewItem>
            </TreeViewItem>
        </TreeView>
    </Grid>
</my:DataSelectWindowBase>
